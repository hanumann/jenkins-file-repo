pipeline{
    agent{label 'master'}
    
    stages{
        stage('git checkout'){
            steps{
                script{
                   git credentialsId: 'git', url: 'https://github.com/hanumann/game-of-life.git' 
                }
            }
        }
        stage ('maven build'){
            steps{
                script{
                    withMaven(jdk: 'jdk8', maven: 'maven1') {
                     sh 'mvn clean deploy'
                }
            }
        }
        }
        
    }
}
